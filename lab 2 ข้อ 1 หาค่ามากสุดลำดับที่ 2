#include <stdio.h>

int main() 
{
    int n1, n2, n3, n4;
    scanf("%d %d %d %d", &n1, &n2, &n3, &n4);

    int max1 = (n1 > n2 ? n1 : n2);
    max1 = (max1 > n3 ? max1 : n3);
    max1 = (max1 > n4 ? max1 : n4);

    int a = (n1 == max1 ? 0 : n1);
    int b = (n2 == max1 ? 0 : n2);
    int c = (n3 == max1 ? 0 : n3);
    int d = (n4 == max1 ? 0 : n4);

    int max2 = (a > b ? a : b);
    max2 = (max2 > c ? max2 : c);
    max2 = (max2 > d ? max2 : d);

    printf("%d", max2);
    return 0;
}
